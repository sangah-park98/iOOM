	<queryString>
		<![CDATA[SELECT 
					b.sn1 AS d1,
					CONCAT_WS('-', 
				        SUBSTRING(b.impCd, 1, 5),
				        SUBSTRING(b.impCd, 6, 2),
				        SUBSTRING(b.impCd, 8)) AS d2,
					b.NabNo AS d3,
					COALESCE(b.tax, 0) AS d4,
					COALESCE(b.vat, 0) AS d5,
					DATE_FORMAT(STR_TO_DATE(a.nabDay, '%Y%m%d'), '%Y년 %m월') AS d6
				FROM vatbil5fza1 a
				LEFT JOIN vatbil5fzb1 b
				ON a.BRANCH = b.BRANCH
				AND a.CUS_PRG = b.CUS_PRG
				AND a.docCode = b.docCode
				WHERE a.impCd = REPLACE($P{i_prm1},'-','')
				AND a.NabSdNo = $P{i_prm2}
				ORDER BY sn1
				LIMIT 25 OFFSET $P{i_prm4}]]>
	</queryString>
