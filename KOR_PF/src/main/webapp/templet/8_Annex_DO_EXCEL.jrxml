	<parameter name="i_prm1" class="java.lang.String"/>
	<parameter name="i_prm2" class="java.lang.String"/>
	<parameter name="i_prm3" class="java.lang.String"/>
	<parameter name="i_prm4" class="java.lang.String"/>
	<parameter name="i_prm5" class="java.lang.String"/>
	<parameter name="i_prm6" class="java.lang.String"/>
	<parameter name="i_prm7" class="java.lang.String"/>
	<parameter name="i_prm8" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT prm1,prm2,prm3,prm4,prm5,prm6,prm7,prm8,prm9,prm10,prm11,prm12,prm13,prm14,prm15,prm16,prm17,prm18,prm19 FROM (
		SELECT 
		IFNULL(C.CMPNY_NM,'') AS prm1,
		IFNULL(C.TAX_NB,'') AS prm2,
		case when H.CUSTOM_EXP_NO IS NOT NULL AND H.CUSTOM_EXP_NO != '' then CONCAT('Tờ khai hải quan xuất khẩu số: ',H.CUSTOM_EXP_NO) 
		ELSE CONCAT('Hóa đơn giá trị gia tăng số: ',H.INVOICE_NO) END AS prm3,
		CONCAT(IFNULL(D.BD_RT,0), ' %') AS prm4,
		CONCAT(IFNULL(E.PRDCT_VNM, ''), ' / ', IFNULL(E.PRDCT_ENM, '')) AS prm5,
		IFNULL(GET_HSCD(A.HS_CD),'') AS prm6,
		IFNULL(E.ITEM_CD,'') AS prm7,
		CONCAT(IFNULL(H.SALES_QTY,0),' ',IFNULL(E.BASE_UNT,'')) AS prm8,
		CONCAT('(',IFNULL(H.INCOTERMS,'FOB'),') :',IFNULL(FORMAT(H.SALES_FOB_PRICE,0),''),' ','VND', ' / ',H.SALES_PRICE,' ',H.CRRNCY_UNT,' ( Tỷ giá : ',H.EXCHNG_RT,')') as prm9,
		CONCAT(IFNULL(D.BD_RT,0), ' %') AS prm10,
		IFNULL(J.PLNT_CITY, '') as prm11,
		DATE_FORMAT(CURDATE(), '%d') prm12,
		DATE_FORMAT(CURDATE(), '%m') prm13,
		DATE_FORMAT(CURDATE(), '%y') prm14,
		FORMAT(H.SALES_FOB_PRICE,0) as prm15,
		FORMAT(I.ofshr_price,0) prm16,
		case when H.CUSTOM_EXP_NO IS NOT NULL AND H.CUSTOM_EXP_NO != '' then CONCAT('Ngày tờ khai xuất khẩu : ', IFNULL(DATE_FORMAT(H.CUSTOM_EXP_DT,'%d/%m/%Y'),DATE_FORMAT(H.SALES_DT,'%d/%m/%Y')) )
		ELSE CONCAT('Ngày hóa đơn : ', DATE_FORMAT(H.SALES_DT,'%d/%m/%Y')) END AS prm19,
		CONCAT(ROUND((H.SALES_FOB_PRICE - I.ofshr_price) / H.SALES_FOB_PRICE * 100, 2),' %') as  prm17,
		case when not A.FTA_CD IN ('UKVFTA', 'EVFTA', 'GSP(EU)','GSP(UK)') AND H.INCOTERMS = 'EXW' AND H.SALES_DELIV_IM != 0 then 
		CONCAT('Tổng chi phí vận chuyển nội địa, v.v.: ',FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0),0),' (Tổng chi phí vận chuyển của lô hàng: ',FORMAT(SUM(K.SALES_DELIV_IM),0),' * Trị giá xuất khẩu của mặt hàng này : ',FORMAT(H.SALES_PRICE_FR,0),' tổng trị giá xuất khẩu của lô hàng: ',FORMAT(SUM(K.SALES_PRICE_FR),0),' )','\n','FOB Price: EXW + Sum of Domestic Transportation Cost: ' ,FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0),0) )
		when A.FTA_CD IN ('UKVFTA', 'EVFTA', 'GSP(EU)','GSP(UK)') AND H.INCOTERMS IN ('FCA','FAS','FOB')  AND H.SALES_DELIV_IM != 0 then
		CONCAT('Tổng chi phí vận chuyển nội địa, v.v.: ',FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0),0),' (Tổng chi phí vận chuyển của lô hàng: ',FORMAT(SUM(K.SALES_DELIV_IM),0),' * Trị giá xuất khẩu của mặt hàng này : ',FORMAT(H.SALES_PRICE_FR,0),' tổng trị giá xuất khẩu của lô hàng: ',FORMAT(SUM(K.SALES_PRICE_FR),0),' )','\n','Giá EXW: Trị giá FOB - Tổng chi phí vận chuyển nội địa: ' ,FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0),0) )
		when A.FTA_CD IN ('UKVFTA', 'EVFTA', 'GSP(EU)','GSP(UK)') AND H.INCOTERMS IN ('CFR','CIF','CIP','CPT','DAP','DPU','DDP')  AND H.SALES_DELIV_IM != 0 AND H.SALES_DELIV_EX != 0 then
		CONCAT('Tổng chi phí vận chuyển nội địa và quốc tế v.v.: ',FORMAT( ( ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0)+ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_EX),0) ) ,0),' (Tổng chi phí vận chuyển của lô hàng: ',FORMAT(SUM(K.SALES_DELIV_IM)+SUM(K.SALES_DELIV_EX),0),' * Trị giá xuất khẩu của mặt hàng này : ',FORMAT(H.SALES_PRICE_FR,0),' tổng trị giá xuất khẩu của lô hàng: ',FORMAT(SUM(K.SALES_PRICE_FR),0),' )','\n','Giá EXW: Trị giá FOB - Tổng chi phí vận chuyển nội địa: ' ,FORMAT( ( ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0)+ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_EX),0) ) ,0) )
		when A.FTA_CD not IN ('UKVFTA', 'EVFTA', 'GSP(EU)','GSP(UK)') AND H.INCOTERMS IN ('CFR','CIF','CIP','CPT','DAP','DPU','DDP') AND H.SALES_DELIV_EX != 0 then
		CONCAT('Tổng chi phí vận chuyển nội địa, v.v.: ',FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_EX),0),0),' (Tổng chi phí vận chuyển của lô hàng: ',FORMAT(SUM(K.SALES_DELIV_IM)+SUM(K.SALES_DELIV_EX),0),' * Trị giá xuất khẩu của mặt hàng này : ',FORMAT(H.SALES_PRICE_FR,0),' tổng trị giá xuất khẩu của lô hàng: ',FORMAT(SUM(K.SALES_PRICE_FR),0),' )','\n','Giá FOB: Trị giá - Tổng chi phí vận chuyển quốc tế ' ,FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_EX),0),0) )
		ELSE ''
 		END AS prm18
	FROM 
		jdgmnt_result A 
		LEFT JOIN cmpny_info C
			ON A.CMPNY_CD = C.CMPNY_CD
		LEFT JOIN psr_info D
			ON A.PSR_SEQ = D.PSR_SEQ
		LEFT JOIN item_info E
			ON ( A.CMPNY_CD = E.CMPNY_CD
			 AND A.PLNT_CD = E.PLNT_CD
			 AND A.ITEM_CD = E.ITEM_CD )
		LEFT JOIN crft_do_hist F
			ON (A.JDGMNT_SEQ = F.JDGMNT_SEQ AND A.PSR_SEQ = F.PSR_SEQ)
		LEFT JOIN sales_list H
			ON (A.SALES_NO = H.SALES_NO AND  A.PLNT_CD = H.PLNT_CD AND A.ITEM_CD = H.ITEM_CD AND A.CMPNY_CD = H.CMPNY_CD AND A.SALES_ORDR = H.SALES_ORDR AND A.STNDRD_NO = H.STNDRD_NO) 
		LEFT JOIN (
		SELECT JDGMNT_SEQ, PSR_SEQ,
		SUBSTR(vat_dts, INSTR(vat_dts, '=')+3, INSTR(vat_dts, 'Buffer')-INSTR(vat_dts, '=')-7) AS actual_ratio
		FROM jdgmnt_psr_info ) G
		ON A.JDGMNT_SEQ = G.JDGMNT_SEQ AND A.PSR_SEQ = G.PSR_SEQ
		LEFT JOIN (
		SELECT PSR_SEQ, SUM(ROUND(ofshr_price,0)) ofshr_price
		FROM jdgmnt_rawmtrl
		WHERE 
		jdgmnt_seq =$P{i_prm1}
		GROUP BY PSR_SEQ ) AS I
		ON A.PSR_SEQ = I.PSR_SEQ
		LEFT JOIN cmpny_plnt_info J
		ON A.plnt_cd = J.plnt_cd AND A.cmpny_cd = J.cmpny_cd
		LEFT JOIN sales_list K 
		ON (A.CMPNY_CD = K.CMPNY_CD AND  A.PLNT_CD = K.PLNT_CD AND A.BUYR_CD = K.BUYR_ID AND A.SALES_NO = K.SALES_NO AND A.SALES_ORDR = K.SALES_ORDR) 
	WHERE 
	A.JDGMNT_SEQ = $P{i_prm1}
	AND F.ISSU_NO = $P{i_prm4}
	AND A.PLNT_CD = $P{i_prm5}
	AND A.PSR_SEQ = $P{i_prm7}
	
	UNION
	
	SELECT 
	IFNULL(C.CMPNY_NM,'') AS prm1,
	IFNULL(C.TAX_NB,'') AS prm2,
	case when H.CUSTOM_EXP_NO IS NOT NULL AND H.CUSTOM_EXP_NO != '' then CONCAT('Tờ khai hải quan xuất khẩu số: ',H.CUSTOM_EXP_NO) 
	ELSE CONCAT('Hóa đơn giá trị gia tăng số: ',H.INVOICE_NO) END AS prm3,
	CONCAT(IFNULL(D.BD_RT,0), ' %') AS prm4,
	CONCAT(IFNULL(E.PRDCT_VNM, ''), ' / ', IFNULL(E.PRDCT_ENM, '')) AS prm5,
	IFNULL(GET_HSCD(A.HS_CD),'') AS prm6,
	IFNULL(E.ITEM_CD,'') AS prm7,
	CONCAT(IFNULL(H.SALES_QTY,0),' ',IFNULL(E.BASE_UNT,'')) AS prm8,
	CONCAT('(',IFNULL(H.INCOTERMS,'FOB'),') :',IFNULL(FORMAT(H.SALES_FOB_PRICE,0),''),' ','VND', ' / ',H.SALES_PRICE,' ',H.CRRNCY_UNT,' ( Tỷ giá : ',H.EXCHNG_RT,')') as prm9,
	CONCAT(IFNULL(D.BD_RT,0), ' %') AS prm10,
	IFNULL(J.PLNT_CITY, '') as prm11,
	DATE_FORMAT(CURDATE(), '%d') prm12,
	DATE_FORMAT(CURDATE(), '%m') prm13,
	DATE_FORMAT(CURDATE(), '%y') prm14,
	FORMAT(H.SALES_FOB_PRICE,0) as prm15,
	FORMAT(I.ofshr_price,0) prm16,
	case when H.CUSTOM_EXP_NO IS NOT NULL AND H.CUSTOM_EXP_NO != '' then CONCAT('Ngày tờ khai xuất khẩu : ', IFNULL(DATE_FORMAT(H.CUSTOM_EXP_DT,'%d/%m/%Y'),DATE_FORMAT(H.SALES_DT,'%d/%m/%Y')) )
	ELSE CONCAT('Ngày hóa đơn : ', DATE_FORMAT(H.SALES_DT,'%d/%m/%Y')) END AS prm19,
	CONCAT(ROUND((H.SALES_FOB_PRICE - I.ofshr_price) / H.SALES_FOB_PRICE * 100, 2),' %') as  prm17,
	case when not A.FTA_CD IN ('UKVFTA', 'EVFTA', 'GSP(EU)','GSP(UK)') AND H.INCOTERMS = 'EXW' AND H.SALES_DELIV_IM != 0 then 
	CONCAT('Tổng chi phí vận chuyển nội địa, v.v.: ',FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0),0),' (Tổng chi phí vận chuyển của lô hàng: ',FORMAT(SUM(K.SALES_DELIV_IM),0),' * Trị giá xuất khẩu của mặt hàng này : ',FORMAT(H.SALES_PRICE_FR,0),' tổng trị giá xuất khẩu của lô hàng: ',FORMAT(SUM(K.SALES_PRICE_FR),0),' )','\n','FOB Price: EXW + Sum of Domestic Transportation Cost: ' ,FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0),0) )
	when A.FTA_CD IN ('UKVFTA', 'EVFTA', 'GSP(EU)','GSP(UK)') AND H.INCOTERMS IN ('FCA','FAS','FOB')  AND H.SALES_DELIV_IM != 0 then
	CONCAT('Tổng chi phí vận chuyển nội địa, v.v.: ',FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0),0),' (Tổng chi phí vận chuyển của lô hàng: ',FORMAT(SUM(K.SALES_DELIV_IM),0),' * Trị giá xuất khẩu của mặt hàng này : ',FORMAT(H.SALES_PRICE_FR,0),' tổng trị giá xuất khẩu của lô hàng: ',FORMAT(SUM(K.SALES_PRICE_FR),0),' )','\n','Giá EXW: Trị giá FOB - Tổng chi phí vận chuyển nội địa: ' ,FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0),0) )
	when A.FTA_CD IN ('UKVFTA', 'EVFTA', 'GSP(EU)','GSP(UK)') AND H.INCOTERMS IN ('CFR','CIF','CIP','CPT','DAP','DPU','DDP')  AND H.SALES_DELIV_IM != 0 AND H.SALES_DELIV_EX != 0 then
	CONCAT('Tổng chi phí vận chuyển nội địa và quốc tế v.v.: ',FORMAT( ( ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0)+ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_EX),0) ) ,0),' (Tổng chi phí vận chuyển của lô hàng: ',FORMAT(SUM(K.SALES_DELIV_IM)+SUM(K.SALES_DELIV_EX),0),' * Trị giá xuất khẩu của mặt hàng này : ',FORMAT(H.SALES_PRICE_FR,0),' tổng trị giá xuất khẩu của lô hàng: ',FORMAT(SUM(K.SALES_PRICE_FR),0),' )','\n','Giá EXW: Trị giá FOB - Tổng chi phí vận chuyển nội địa: ' ,FORMAT( ( ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_IM),0)+ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_EX),0) ) ,0) )
	when A.FTA_CD not IN ('UKVFTA', 'EVFTA', 'GSP(EU)','GSP(UK)') AND H.INCOTERMS IN ('CFR','CIF','CIP','CPT','DAP','DPU','DDP') AND H.SALES_DELIV_EX != 0 then
	CONCAT('Tổng chi phí vận chuyển nội địa, v.v.: ',FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_EX),0),0),' (Tổng chi phí vận chuyển của lô hàng: ',FORMAT(SUM(K.SALES_DELIV_IM)+SUM(K.SALES_DELIV_EX),0),' * Trị giá xuất khẩu của mặt hàng này : ',FORMAT(H.SALES_PRICE_FR,0),' tổng trị giá xuất khẩu của lô hàng: ',FORMAT(SUM(K.SALES_PRICE_FR),0),' )','\n','Giá FOB: Trị giá - Tổng chi phí vận chuyển quốc tế ' ,FORMAT(ROUND(H.SALES_PRICE_FR/SUM(K.SALES_PRICE_FR)* SUM(K.SALES_DELIV_EX),0),0) )
	ELSE ''
	END AS prm18
	FROM 
		jdgmnt_result A 
		LEFT JOIN cmpny_info C
		ON A.CMPNY_CD = C.CMPNY_CD
		LEFT JOIN psr_info D
		ON A.PSR_SEQ = D.PSR_SEQ
		LEFT JOIN item_info E
		ON ( A.CMPNY_CD = E.CMPNY_CD
			 AND A.PLNT_CD = E.PLNT_CD
			 AND A.ITEM_CD = E.ITEM_CD )
		LEFT JOIN crft_co_hist_dts F
		ON (A.JDGMNT_SEQ = F.JDGMNT_SEQ AND A.PSR_SEQ = F.PSR_SEQ)
		LEFT JOIN sales_list H
		ON (A.SALES_NO = H.SALES_NO AND  A.PLNT_CD = H.PLNT_CD AND A.ITEM_CD = H.ITEM_CD AND A.CMPNY_CD = H.CMPNY_CD AND A.SALES_ORDR = H.SALES_ORDR AND A.STNDRD_NO = H.STNDRD_NO) 
		LEFT JOIN (
		SELECT JDGMNT_SEQ, PSR_SEQ,
		SUBSTR(vat_dts, INSTR(vat_dts, '=')+3, INSTR(vat_dts, 'Buffer')-INSTR(vat_dts, '=')-7) AS actual_ratio
		FROM jdgmnt_psr_info ) G
		ON A.JDGMNT_SEQ = G.JDGMNT_SEQ AND A.PSR_SEQ = G.PSR_SEQ
		LEFT JOIN (
		SELECT PSR_SEQ, SUM(ROUND(ofshr_price,0)) ofshr_price
		FROM jdgmnt_rawmtrl
		WHERE 
		jdgmnt_seq = $P{i_prm1}
		GROUP BY PSR_SEQ ) AS I
		ON A.PSR_SEQ = I.PSR_SEQ
		LEFT JOIN cmpny_plnt_info J
		ON A.plnt_cd = J.plnt_cd AND A.cmpny_cd = J.cmpny_cd
		LEFT JOIN sales_list K 
		ON (A.CMPNY_CD = K.CMPNY_CD AND  A.PLNT_CD = K.PLNT_CD AND A.BUYR_CD = K.BUYR_ID AND A.SALES_NO = K.SALES_NO AND A.SALES_ORDR = K.SALES_ORDR) 
	WHERE 
	F.JDGMNT_SEQ = $P{i_prm1}
	AND F.ISSU_NO = $P{i_prm4}
	AND F.PLNT_CD = $P{i_prm5}
	AND A.PSR_SEQ = $P{i_prm7}
	) A WHERE 1=1 AND prm1 !='']]>
	</queryString>
