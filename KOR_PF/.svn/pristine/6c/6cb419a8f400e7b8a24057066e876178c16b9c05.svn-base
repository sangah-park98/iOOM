// 스크롤할 때 행이 자동으로 추가 로드될 수 있게 하는 함수
function fn_expViewasEventReg(){
	
   $("#customExportViewTable .wtHolder").scroll(function(){
	  //fn_expViewFileList(row, col)
  	  var scrollTop = $("#customExportViewTable .wtHolder").scrollTop();
  	  var countPerPage = $("#customsExportViewPageCnt option:selected").val();
  	  var rowHeight = customsExportViewHot.getRowHeight();

  	  var addCnt = 790;
  	  if(countPerPage == "50"){
  		  addCnt = 790;
  	  }else if(countPerPage == "100"){
  		  addCnt = 2040;
  	  }else if(countPerPage == "200"){
  		  addCnt = 3290;
  	  }else if(countPerPage == "500"){
  		  addCnt = 4540;
  	  }
  	  if(customsExportViewScrollTp && customExportViewIndex != 9999 && scrollTop >= (countPerPage * customExportViewIndex * rowHeight) + addCnt){
  		  fn_exportViewScroll();
  	  }
   });
}

// 스크롤
function fn_exportViewScroll(){
	if( $("input[name=exportViewType]:checked").val() == "enrol")
		return;
	fn_loading(true);
	customsExportViewScrollTp = false;
	customExportViewIndex++;

	$.ajax({
		type : "POST",
		url : "/customsExport/selectExportViewList.do",
		data : fn_setExportViewForm(),
		beforeSend : function(xmlHttpRequest){
			xmlHttpRequest.setRequestHeader("AJAX", "true");
		},
		dataType: "json",
        success : function(data) {
        	var getData = customsExportViewHot.getSourceData();
        	var meargeJson = getData.concat(data.resultList);
        	customsExportViewHot.loadData(meargeJson);
        	customsExportViewScrollTp = true;
        	fn_loading(false);
        },
        error : function(e, textStatus, errorThrown) {
        	if(e.status == 400){
        		alert("Your request is up. Please log back in if you wish continue");
        		location.href = document.referrer;
        	} else {
	        	console.log(errorThrown);
	        	alert(msgSearchError);
        	}
        }
	});
}

// 검색
function fn_searchExportView(){
	customExportViewIndex = 0;
	$("#expViewSrch2").val($("#customsExportView_srch2").val());
	$("#expViewSrch3").val($("#customsExportView_srch3").val());
	$("#expViewSrch4").val($("#customsExportView_srch4").val());
	$("#expViewSrch5").val($("#customsExportView_srch5").val());
	$("#expViewSrch6").val($("#exportView_srch6").val());
	$("#expViewSrch7").val($("#exportView_srch7").val());
	
	var data = fn_setExportViewForm();
	var valid = fn_validateSearchDate(data["srch2"], data["srch3"]);

	if(valid === "false"){
		data["srch2"] = null;
		data["srch3"] = null;
		$("#customsExportView_srch2").val("");
		$("#customsExportView_srch3").val("");
		return;
	} else {
		data["srch2"] = $("#customsExportView_srch2").val();
		data["srch3"] = $("#customsExportView_srch3").val();
	}
	
	if(data["srch2"] == null || data["srch2"] == "" || data["srch3"] == "" || data["srch3"] == null){
		alert("날짜를 입력해 주세요.");
		return;
	}
	
	fn_loading(true);

	$.ajax({
		type : "POST",
		url : "/customsExport/selectExportViewList.do",
		data : fn_setExportViewForm(),
		beforeSend : function(xmlHttpRequest){
			xmlHttpRequest.setRequestHeader("AJAX", "true");
		},
		dataType: "json",
        success : function(data) {
        	customsExportViewHot.loadData([]);
    		customsExportViewSpecHot.loadData([]);
    		customsExportViewLanHot.loadData([]);
        	customsExportViewHot.loadData(data.resultList);
        	var totCnt = (data.resultList.length > 0) ? data.resultList[0].cnt : 0;
        	$("#exportViewCnt").text(totCnt); 
    	fn_loading(false);
	    },
	    error : function(e, textStatus, errorThrown) {
	    	if(e.status == 400){
	    		alert("Your request is up. Please log back in if you wish continue");
	    		location.href = document.referrer;
	    	} else {
	        	console.log(errorThrown);
	        	alert(msgSearchError);
	    	}
	    }
	});
};


