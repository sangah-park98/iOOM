    var today = date.toISOString().substring(0, 10);
    var oneYearAgoFormatted = oneYearAgo.toISOString().substring(0, 10);

    $("#deadLineView_srch2").val(oneYearAgoFormatted);
    $("#deadLineView_srch3").val(today);
}

