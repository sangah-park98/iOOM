		    CASE WHEN #{lang} = 'en' THEN 'IT Representative' ELSE 'IT 담당자' END AS usrType,
		    CASE 
		        WHEN C.tax_no IN ('2118180957','3128109408','3128103345','1298118421') THEN '권예지'
		        ELSE '박상아'
		    END AS usrNm,
		    CASE 
		        WHEN C.tax_no IN ('2118180957','3128109408','3128103345','1298118421') THEN 'yjkwon@customsservice.co.kr'
		        ELSE 'sapark@customsservice.co.kr'
		    END AS usrEmail,
		    CASE 
		        WHEN C.tax_no IN ('2118180957','3128109408','3128103345','1298118421') THEN '070-4343-7729'
		        ELSE '070-4343-7732'
		    END AS usrTel
		FROM cmpny_charge_info C
		WHERE 1=1
		<if test="list != null and list.size() > 0">
            AND  C.tax_no IN 
            <foreach item="id" collection="list" open="(" separator="," close=")">
                #{id}
            </foreach>
       	</if> -->
    	
		) A 
		WHERE usrNm != ''
		ORDER BY CMPNY_NM
		,
		CASE usrType
	    WHEN '임원' THEN 1
	    WHEN 'Executive' THEN 1
	    WHEN '전담관세사' THEN 2
	    WHEN 'Dedicated Customs Broker' THEN 2
	    WHEN '통관담당자' THEN 3
	    WHEN 'Customs Representative' THEN 3
	   <!--  WHEN 'IT담당자' THEN 4
	    WHEN 'IT Representative' THEN 4 -->
	    ELSE 4
	  END,
	  CMPNY_NM
	</select>
</mapper>