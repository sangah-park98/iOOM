<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egovframework.pf.rpt.service.impl.reportAnalysisMapper">

	<select id="selectImportStatus" parameterType="searchVO" resultType="egovMap">
			WITH CTE AS (
			SELECT
			    DATE_FORMAT(Lis_Day, '%Y-%m-%d')  AS LIS_DAY
			  , COUNT(*) AS CNT
			  , SUM(Tot_Tax_Krw) AS AMOUNT_KRW
			  , SUM(CASE WHEN EXC_DIVI_MARK ='11' THEN 1 ELSE 0 END) AS TX_TYPE1 -- 일반형태수입
			  , SUM(CASE WHEN EXC_DIVI_MARK ='94' THEN 1 ELSE 0 END) AS TX_TYPE2 -- 기타수입승인면제
			  , SUM(CASE WHEN EXC_DIVI_MARK ='87' THEN 1 ELSE 0 END) AS TX_TYPE3 -- 견품및광고용품
			  , SUM(CASE WHEN EXC_DIVI_MARK ='93' THEN 1 ELSE 0 END) AS TX_TYPE4 -- 수출물품대체용
			  , SUM(CASE WHEN EXC_DIVI_MARK IN ('83', '29', '89', '84', '54','86','88','90','21','85','92','96') THEN 1 ELSE 0 END) AS TX_TYPE5 -- 기타
			  , SUM(CASE WHEN Rece_Result LIKE '%제출%' THEN 1 ELSE 0 END) AS TX_PRC2
			  , SUM(CASE WHEN Rece_Result LIKE '%검사%' THEN 1 ELSE 0 END) AS TX_PRC3
			    FROM CUSDEC929A1
			    WHERE  LIS_DAY BETWEEN #{srch1} AND #{srch2}
			    AND  RPT_SEQ = '00'
			    <if test="list != null and list.size() > 0">
		            AND NAB_SDNO IN 
		            <foreach item="id" collection="list" open="(" separator="," close=")">
		                #{id}
		            </foreach>
		        </if>
			    GROUP BY LIS_DAY
			),
			SUMS AS (       
	    SELECT
	        SUM(CNT)        AS S_CNT,
	        SUM(AMOUNT_KRW) AS S_AMOUNT_KRW,
	        SUM(TX_TYPE1)   AS S_TYPE1,
	        SUM(TX_TYPE2)   AS S_TYPE2,
	        SUM(TX_TYPE3)   AS S_TYPE3,
	        SUM(TX_TYPE4)   AS S_TYPE4,
	        SUM(TX_TYPE5)   AS S_TYPE5,
	        SUM(TX_PRC2)    AS S_PRC2,
	        SUM(TX_PRC3)    AS S_PRC3
	    FROM CTE
		)
		
		SELECT
		    LIS_DAY,
		    CNT,
		    AMOUNT_KRW,
		    TX_TYPE1,
		    TX_TYPE2,
		    TX_TYPE3,
		    TX_TYPE4,
		    TX_TYPE5,
		    CNT - TX_PRC2 - TX_PRC3 AS TX_PRC1,
		    TX_PRC2,
		    TX_PRC3
		FROM CTE
		
		UNION ALL
		-- ② “합계” 행
		SELECT
		    '합계',
		    S_CNT,
		    S_AMOUNT_KRW,
		    S_TYPE1,
		    S_TYPE2,
		    S_TYPE3,
		    S_TYPE4,
		    S_TYPE5,
		    S_CNT - S_PRC2 - S_PRC3,
		    S_PRC2,
		    S_PRC3
		FROM SUMS
		
		UNION ALL
		SELECT
		    '비율(%)',                         
		    '',                                
		    '',                               
		    CONCAT(ROUND(S_TYPE1 / NULLIF(S_TYPE1+S_TYPE2+S_TYPE3+S_TYPE4+S_TYPE5,0)*100, 2),'%'),
		    CONCAT(ROUND(S_TYPE2 / NULLIF(S_TYPE1+S_TYPE2+S_TYPE3+S_TYPE4+S_TYPE5,0)*100, 2),'%'),
		    CONCAT(ROUND(S_TYPE3 / NULLIF(S_TYPE1+S_TYPE2+S_TYPE3+S_TYPE4+S_TYPE5,0)*100, 2),'%'),
		    CONCAT(ROUND(S_TYPE4 / NULLIF(S_TYPE1+S_TYPE2+S_TYPE3+S_TYPE4+S_TYPE5,0)*100, 2),'%'),
		    CONCAT(ROUND(S_TYPE5 / NULLIF(S_TYPE1+S_TYPE2+S_TYPE3+S_TYPE4+S_TYPE5,0)*100, 2),'%'),
		    CONCAT(ROUND((S_CNT-S_PRC2-S_PRC3) / NULLIF(S_CNT,0)*100, 2),'%'),
		    CONCAT(ROUND(S_PRC2 / NULLIF(S_CNT,0)*100, 2),'%'),
		    CONCAT(ROUND(S_PRC3 / NULLIF(S_CNT,0)*100, 2),'%')
		FROM SUMS
		;
	</select>
	
	<select id="selectTaxStatus" parameterType="searchVO" resultType="egovMap">
			WITH TAX AS (               
			    SELECT
			        DATE_FORMAT(Lis_Day,'%Y-%m-%d') AS LIS_DAY,
			        COUNT(*)                       AS Cnt_Imp,
			        SUM(Tot_Gs)                    AS Tot_Gs,
			        SUM(Tot_Vat)                   AS Tot_Vat,
			        SUM(Tot_Ts)                    AS Tot_Ts, -- 개소세
			        SUM(Tot_Gt)                    AS Tot_Gt, -- 교통세
			        SUM(Tot_Hof)                   AS Tot_Hof, -- 주세
			        SUM(Tot_Ky)                    AS Tot_Ky, -- 교육세
			        SUM(Tot_Nt)                    AS Tot_Nt, -- 농특세
			        SUM(Tot_Add_Tax + Tot_Dly_Tax) AS Tot_Add_Tax,
			        SUM(Tot_Vat +Tot_Gs + Tot_Ts + Tot_Gt + Tot_Ky + Tot_Nt + Tot_Add_Tax + Tot_Dly_Tax)  AS Tot_Tax_Sum
			    FROM cusdec929a1
			WHERE Lis_Day BETWEEN #{srch1} AND #{srch2}
			    AND RPT_SEQ = '00'
		    <if test="list != null and list.size() > 0">
	            AND NAB_SDNO IN 
	            <foreach item="id" collection="list" open="(" separator="," close=")">
	                #{id}
	            </foreach>
	        </if>
			GROUP BY Lis_Day
			)
			SELECT
			    LIS_DAY,
			    Cnt_Imp,
			    Tot_Gs,
			    Tot_Vat,
			    Tot_Ts,
			    Tot_Gt,
			    Tot_Hof,
			    Tot_Ky,
			    Tot_Nt,
			    Tot_Add_Tax,
			    Tot_Tax_Sum
			FROM TAX
			
			UNION ALL 
			
			SELECT 
			
			'합계' AS LIS_DAY
			,SUM(Cnt_Imp)
			,SUM(Tot_Gs)
			,SUM(Tot_vat)
			,SUM(Tot_Ts)
			,SUM(Tot_Gt)
			,SUM(Tot_Hof)
			,SUM(Tot_Ky)
			,SUM(Tot_Nt)
			,SUM(Tot_Add_Tax)
			,SUM(Tot_Tax_Sum)
			FROM TAX

			ORDER BY Lis_Day;
	</select>
	
	<select id="selectTaxCount" parameterType="searchVO" resultType="egovMap">
			  SELECT		
			    SUBSTRING(LIS_DAY,1,6)   AS LIS_DAY
				 , COUNT(*) AS Cnt_Imp
				 , SUM(Tot_Tax_Krw)  AS  Amount_Imp
				FROM CUSDEC929A1
			    WHERE  1=1	
			      AND SUBSTRING(LIS_DAY,1,6) &gt;= #{srch3}	
			      AND SUBSTRING(LIS_DAY,1,6) &lt;= #{srch4}	
			      AND  RPT_SEQ = '00'	
			      <if test="list != null and list.size() > 0">
		            AND NAB_SDNO IN 
		            <foreach item="id" collection="list" open="(" separator="," close=")">
		                #{id}
		            </foreach>
		        </if>
			    GROUP BY SUBSTRING(LIS_DAY,1,6) 	
	</select>
	
	
	<select id="selectHscodeList" parameterType="searchVO" resultType="egovMap">
			 SELECT D.HS
		        FROM CUSDEC929A1 C 
		        LEFT OUTER JOIN CUSDEC929B1 D 
		            ON C.SN = D.SNA1
		            AND C.BRANCH = D.BRANCH
						AND C.CUS_PRG = D.CUS_PRG
						AND C.RPT_NO = D.RPT_NO
		        WHERE  
		            C.Lis_Day BETWEEN #{srch1} AND #{srch2}
		            AND C.Rpt_Seq = '00'
	            <if test="list != null and list.size() > 0">
		            AND C.Nab_SdNo IN 
		            <foreach item="id" collection="list" open="(" separator="," close=")">
		                #{id}
		            </foreach>
		        </if>
		           GROUP BY D.HS
		        ORDER BY SUM(D.Tax_Krw) DESC
		        LIMIT 10
	</select>
	
	<select id="selectTaxList" parameterType="searchVO" resultType="egovMap">
		WITH HS AS (	 
			 SELECT 
				    ROW_NUMBER() OVER (ORDER BY SUM(B.Tax_Krw) DESC, B.HS, B.Gs_Divi) AS row_num,
				    CONCAT(SUBSTRING(B.HS, 1, 4), '.', SUBSTRING(B.HS, 5, 2), '-', SUBSTRING(B.HS, 7, 4)) AS hs,
				    RTRIM(B.Gs_Divi) AS Gs_Divi,
				    MAX(CONCAT(B.Exc_GName1, B.Exc_GName2)) AS Exc_GName, 
				    MAX(B.Gs_Rate) AS Gs_Rate,
				    SUM(B.Tax_Krw) AS Tax_Krw,
				    SUM(B.Gs) AS Gs
				FROM CUSDEC929A1 A
				LEFT OUTER JOIN CUSDEC929B1 B 
				    ON A.SN = B.SNA1
				    AND A.RPT_NO = B.RPT_NO
				WHERE  
				    A.Lis_Day BETWEEN #{srch1} AND #{srch2}
				    AND A.Rpt_Seq = '00'
				    <if test="list != null and list.size() > 0">
			            AND A.Nab_SdNo IN 
			            <foreach item="id" collection="list" open="(" separator="," close=")">
			                #{id}
			            </foreach>
		        	</if>
				    <if test="list2 != null and list2.size() > 0">
			            AND B.HS IN 
			            <foreach item="hscode" collection="list2" open="(" separator="," close=")">
			                #{hscode}
			            </foreach>
			        </if>
				GROUP BY B.HS,B.Gs_Divi,B.Gs_Rate
				ORDER BY 
				    SUM(B.Tax_Krw) DESC, 
				    B.HS, 
				    B.Gs_Divi
				LIMIT 10
			)
			SELECT 
				row_num,
				hs,
				Gs_Divi,
				Exc_GName,
				Gs_Rate,
				Tax_Krw,
				Gs
				FROM HS
				
				UNION ALL 
				
			SELECT
				'합계'
				,''
				,''
				,''
				,''
				,SUM(Tax_Krw)
				,SUM(Gs)
				FROM HS
	</select>
	
	<select id = "selectTaxRate" parameterType="searchVO" resultType="egovMap">
		
		WITH GS AS (
			SELECT
				  B.Gs_Divi AS Gs_Divi
				, (SELECT Cont FROM CDSTD WHERE Divi = '관세율구분' AND Code = B.Gs_Divi LIMIT 1) AS Gs_Rate
				, B.Ori_St_Mark1 as Ori_St_Mark1
				, COUNT(*) AS Cnt_Ran
				FROM CUSDEC929A1 A
					LEFT OUTER JOIN CUSDEC929B1 B 
					ON A.SN = B.SNA1
					AND A.BRANCH = B.BRANCH
					AND A.CUS_PRG = B.CUS_PRG
					AND A.RPT_NO = B.RPT_NO
				WHERE  A.Lis_Day BETWEEN #{srch1} AND #{srch2}
					AND  A.Rpt_Seq = '00'
			      	<if test="list != null and list.size() > 0">
		            AND A.Nab_SdNo IN 
		            <foreach item="id" collection="list" open="(" separator="," close=")">
		                #{id}
		            </foreach>
	        		</if>
					AND  ((B.Gs_Divi LIKE 'F%') OR (B.Gs_Divi = 'A') OR (B.Gs_Divi like 'C%') OR (B.Gs_Divi = 'R'))
				GROUP BY B.Gs_Divi
		)
		SELECT *
			FROM (
			    SELECT
			        Gs_Divi,
			        Gs_Rate,
			        Ori_St_Mark1,
			        Cnt_Ran,
			        CONCAT(ROUND(Cnt_Ran / SUM(Cnt_Ran) OVER() * 100 ,2), '%') AS Cnt_Rate,
			        0 AS ord
			    FROM GS
			
			    UNION ALL
			
			    SELECT
			        '',
			        '합계',
			        '',
			        SUM(Cnt_Ran),
			        '100%',
			        1
			    FROM GS
			) t
			ORDER BY
			  ord,                    
			  Cnt_Ran DESC,
			  Gs_Divi,
			  Ori_St_Mark1;
	
	</select>
	
	
	<select id = "selectImportUpdateList" parameterType="searchVO" resultType="egovMap">
		SELECT
		    ROW_NUMBER() OVER (ORDER BY B.SN DESC) AS row_num,
			 CASE WHEN A.Imp_Rpt_No != '' THEN 
			 CONCAT_WS('-', 
		        SUBSTRING(A.Imp_Rpt_No, 1, 5),
		        SUBSTRING(A.Imp_Rpt_No, 6, 2),
		        SUBSTRING(A.Imp_Rpt_No, 8)
		        ) ELSE '' 
		        END AS Imp_Rpt, -- 신고번호
		    DATE_FORMAT(A.Rpt_Day, '%Y-%m-%d') AS Rpt_Day,
			DATE_FORMAT(A.pet_day, '%Y-%m-%d') AS pet_day,
			DATE_FORMAT(B.Rpt_Seq,' %Y-%m-%d') AS Rpt_Seq,
		    CASE 
		        WHEN A.reason_doc1 = '' THEN Rsult_Txt 
		        ELSE A.Reason_Doc1 
		    END AS reason_doc,
			A.Rsult_Mark AS Rsult_Mark,
			CASE WHEN A.Rsult_Mark IN ('99','01','02','03','04')
			THEN '신고인'
			ELSE ''
			END AS reason 
		FROM
		    CUSDMR5FEA1 A
		    INNER JOIN CUSDEC929A1 B 
		    
		    ON A.Imp_Rpt_No = B.Rpt_No 
		    AND B.Rpt_Seq = '00'
		WHERE
		    A.rpt_day BETWEEN #{srch1} AND #{srch2}
		    AND A.Reason_Code != '27'
			 <if test="list != null and list.size() > 0">
	            AND B.Nab_SdNo IN 
	            <foreach item="id" collection="list" open="(" separator="," close=")">
	                #{id}
	            </foreach>
	       	</if>	   
		ORDER BY 
			B.SN DESC;
	</select>
	
	<select id = "selectFtaAmoutList" parameterType="searchVO" resultType="egovMap">
		SELECT 
			ROW_NUMBER() OVER (ORDER BY B.RPT_NO) AS seq,
			CONCAT(A.Reason_Code, '(FTA 사후적용)') AS reasonCode, -- 정정사유코드
			CASE WHEN B.RPT_NO != '' THEN CONCAT_WS('-', 
	        SUBSTRING(B.RPT_NO, 1, 5),
	        SUBSTRING(B.RPT_NO, 6, 2),
	        SUBSTRING(B.RPT_NO, 8)) ELSE '' END AS rpt_no, -- 신고번호
			CASE WHEN A.IMP_RPT_DAY != '' THEN DATE_FORMAT(A.IMP_RPT_DAY, '%Y-%m-%d') ELSE '' END AS imp_rpt_day, -- 신고일자
			CASE WHEN A.RPT_DAY != '' THEN DATE_FORMAT(A.RPT_DAY, '%Y-%m-%d') ELSE '' END AS rpt_day, -- 정정신청일
		   CASE WHEN A.Pet_Day != '' THEN DATE_FORMAT(A.Pet_Day, '%Y-%m-%d') ELSE '' END AS pet_day, -- 승인일자 
			C.CMMN_NM AS CUS, -- 수입세관
			ABS(A.Trans_Tax_Sum) AS Trans_Tax_Sum, -- 증감내역
			ABS(SUM(CASE WHEN D.Edit_Tax_Cot = '관세' THEN D.TRANS_TAX ELSE 0 END)) AS taxCot, -- 관세증감내역
   			ABS(SUM(CASE WHEN D.Edit_Tax_Cot = '부가세' THEN D.TRANS_TAX ELSE 0 END)) AS taxCotVat -- 부가세 증감내역 
		FROM 
			cusdmr5fea1 A
			LEFT JOIN cusdec929a1 B
			ON B.SN = A.CUSDEC929SN
			AND A.IMP_RPT_NO = B.Rpt_No
			AND B.Rpt_Seq = '00'
			AND A.Reason_Code = '27'
			LEFT JOIN adjust_master C 
			ON B.Cus = C.CMMN_CD AND GRP_CD = '세관'
			LEFT JOIN cusdmr5feb2 D 
			ON a.sn = d.sna1 AND a.branch = d.branch AND a.cus_prg = d.cus_prg
			WHERE 1=1
 			 <if test="list != null and list.size() > 0">
	            AND B.Nab_SdNo IN 
	            <foreach item="id" collection="list" open="(" separator="," close=")">
	                #{id}
	            </foreach>
	       	</if>
			 AND B.rpt_day BETWEEN #{srch1} AND #{srch2}
		GROUP BY 
    		B.RPT_NO, A.imp_rpt_day, A.rpt_day, C.CMMN_NM, A.Reason_Code, pet_day, A.Trans_Tax_Sum;
	</select>
	
	
	<select id = "selectNationSales" parameterType="searchVO" resultType="egovMap">
		SELECT 
			Sup_Firm,
			Sup_St_Sht,
			COUNT(*) AS tot_imp,
			sum(Con_Tot_Amt) as conTotAmt,
			sum(Tot_Gs) AS totGs
		FROM cusdec929a1 a
		WHERE 1=1
		 <if test="list != null and list.size() > 0">
           AND Nab_SdNo IN 
           <foreach item="id" collection="list" open="(" separator="," close=")">
               #{id}
           </foreach>
      	</if>
		AND rpt_day BETWEEN #{srch1} AND #{srch2}
		GROUP BY Sup_Firm,Sup_St_Sht
		ORDER BY sum(Con_Tot_Amt) desc,COUNT(*) desc
		;
	</select>
	
	<select id = "avgRate" parameterType="searchVO" resultType="String">
			 	SELECT 
				  IFNULL(CAST(ROUND(AVG(Gs_Rate), 2) AS CHAR),0) AS avgRate
				FROM CUSDEC929A1 A
				LEFT OUTER JOIN CUSDEC929B1 B 
				    ON A.SN = B.SNA1
				    AND A.Rpt_No = B.Rpt_No
				WHERE  
				    A.Lis_Day BETWEEN #{srch1} AND #{srch2}
				    AND A.Rpt_Seq = '00'
	   			 <if test="list != null and list.size() > 0">
		            AND A.Nab_SdNo IN 
		            <foreach item="id" collection="list" open="(" separator="," close=")">
		                #{id}
		            </foreach>
		       	</if>
	</select>
	
		<select id = "topAvgRate" parameterType="searchVO" resultType="String">
			 	SELECT 
				IFNULL(CAST(ROUND(AVG(Gs_Rate), 2) AS CHAR),0) AS avgTopRate
				FROM (
				    SELECT B.Gs_Rate
				    FROM CUSDEC929A1 A
				    LEFT OUTER JOIN CUSDEC929B1 B 
				        ON A.SN = B.SNA1
				        AND A.Rpt_No = B.Rpt_No
				    WHERE  
				        A.Lis_Day BETWEEN #{srch1} AND #{srch2}
				        AND A.Rpt_Seq = '00'
	   	   			 <if test="list != null and list.size() > 0">
			            AND A.Nab_SdNo IN 
			            <foreach item="id" collection="list" open="(" separator="," close=")">
			                #{id}
			            </foreach>
			       	</if>
				    GROUP BY B.HS, B.Gs_Divi, B.Gs_Rate
				    ORDER BY 
				        SUM(B.Tax_Krw) DESC, 
				        B.HS, 
				        B.Gs_Divi
				    LIMIT 10
				) AS topResults;
	</select>
	
	<select id="selectExportStatus" parameterType="searchVO" resultType="egovMap">
		WITH CTX AS (
				SELECT
				    DATE_FORMAT(Exp_Lis_Day, '%Y-%m-%d')  AS Exp_Lis_Day
				  , COUNT(*) AS EXP_CNT
				  , SUM(Tot_Rpt_Krw) AS EXP_AMOUNT_KRW
				  , SUM(CASE WHEN Exc_Divi ='11' THEN 1 ELSE 0 END) AS EX_TYPE1 -- 일반형태
				  , SUM(CASE WHEN Exc_Divi ='29' THEN 1 ELSE 0 END) AS EX_TYPE2 -- 위탁가공원자재
				  , SUM(CASE WHEN Exc_Divi ='94' THEN 1 ELSE 0 END) AS EX_TYPE3 -- 기타수출승인면제
				  , SUM(CASE WHEN Exc_Divi ='92' THEN 1 ELSE 0 END) AS EX_TYPE4 -- 무상견품
				  , SUM(CASE WHEN Exc_Divi IN ('83','72','86','79','78','84','90','93','40','85','89','39','21') THEN 1 ELSE 0 END) AS EX_TYPE5
				  , SUM(CASE WHEN Res961 = '' THEN 1 ELSE 0 END) AS EX_PRC1
				  , SUM(CASE WHEN Res961 in ('15', '18') THEN 1 ELSE 0 END) AS EX_PRC3
				    FROM CUSDEC830A1
				     WHERE  Exp_Lis_Day BETWEEN #{srch1} AND #{srch2}
				    AND  RPT_SEQ = '00'
	   	   			<if test="list != null and list.size() > 0">
		            AND Exp_SdNo IN 
		            <foreach item="id" collection="list" open="(" separator="," close=")">
		                #{id}
		            </foreach>
			       	</if>
				    GROUP BY Exp_Lis_Day
				),
				ESUM AS (
				SELECT 
					SUM(EXP_CNT) AS E_CNT,
					SUM(EXP_AMOUNT_KRW) AS E_EXP_AMOUNT_KRW,
					SUM(EX_TYPE1) AS E_TYPE1,
					SUM(EX_TYPE2) AS E_TYPE2,
					SUM(EX_TYPE3) AS E_TYPE3,
					SUM(EX_TYPE4) AS E_TYPE4,
					SUM(EX_TYPE5) AS E_TYPE5,
					SUM(EX_PRC1) AS E_PRC1,
					SUM(EXP_CNT) - SUM(EX_PRC1) - SUM(EX_PRC3) AS E_PRC2,
					SUM(EX_PRC3) AS E_PRC3
	
				FROM CTX 
				)
				
				SELECT 	  Exp_Lis_Day
							, EXP_CNT
							, EXP_AMOUNT_KRW
							, EX_TYPE1
							, EX_TYPE2
							, EX_TYPE3
							, EX_TYPE4
							, EX_TYPE5
							, EX_PRC1
							, EX_PRC3
							, EXP_CNT - EX_PRC1 - EX_PRC3 AS EX_PRC2
							
				FROM CTX			
			
			UNION ALL 
			 -- 합계
			 
			 SELECT 
			  '합계',
			  E_CNT,
			  E_EXP_AMOUNT_KRW,
			  E_TYPE1,
			  E_TYPE2,
			  E_TYPE3,
			  E_TYPE4,
			  E_TYPE5,
			  E_PRC1,
			  E_PRC3,
			  E_PRC2
			FROM ESUM
			
			UNION ALL
			-- 비율 
			SELECT 
			'비율(%)',
			'',
			'',
			CONCAT(ROUND(E_TYPE1 / NULLIF(E_TYPE1+E_TYPE2+E_TYPE3+E_TYPE4+E_TYPE5,0)*100, 2),'%'),
			CONCAT(ROUND(E_TYPE2 / NULLIF(E_TYPE1+E_TYPE2+E_TYPE3+E_TYPE4+E_TYPE5,0)*100, 2),'%'),
			CONCAT(ROUND(E_TYPE3 / NULLIF(E_TYPE1+E_TYPE2+E_TYPE3+E_TYPE4+E_TYPE5,0)*100, 2),'%'),
			CONCAT(ROUND(E_TYPE4 / NULLIF(E_TYPE1+E_TYPE2+E_TYPE3+E_TYPE4+E_TYPE5,0)*100, 2),'%'),
			CONCAT(ROUND(E_TYPE5 / NULLIF(E_TYPE1+E_TYPE2+E_TYPE3+E_TYPE4+E_TYPE5,0)*100, 2),'%'),
			CONCAT(ROUND(E_PRC1 / NULLIF(E_PRC1+E_PRC2+E_PRC3,0)*100, 2),'%'),
			CONCAT(ROUND(E_PRC3 / NULLIF(E_PRC1+E_PRC2+E_PRC3,0)*100, 2),'%'),
			CONCAT(ROUND(E_PRC2 / NULLIF(E_PRC1+E_PRC2+E_PRC3,0)*100, 2),'%')
			
			FROM ESUM			
				
	</select>
	
	
	<select id="selectNationSalesExport" parameterType="searchVO" resultType="egovMap">
		SELECT
			Buy_Firm,
			Ta_St_isoNm,
			COUNT(*) AS cmtExp,
			sum(Tot_Rpt_Krw) totRptKrw
		FROM cusdec830a1
			WHERE RPT_SEQ = '00'
	 		<if test="list != null and list.size() > 0">
	           AND Exp_SdNo IN 
