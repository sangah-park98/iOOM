	<select id="selectFtaSavingList" parameterType="searchVO" resultType="egovMap">
		WITH base AS (
			    SELECT
			        (SELECT Cont FROM CDSTD WHERE Divi = '관세율구분' AND Code = B.Gs_Divi LIMIT 1) AS Gs_Rate,
			        MAX(B.GS_DIVI) AS GS_DIVI,
			        COUNT(DISTINCT A.RPT_NO) AS CNT,
			        ROUND(SUM(B.TAX_KRW * C.DUTY_RATE)/100, 0) AS FTA_RATE,
			        ROUND(SUM(B.GS),0) AS GS,
			        ABS(ROUND(SUM(B.GS - ((B.TAX_KRW * C.DUTY_RATE) / 100)), 0)) AS DUTY
			    FROM cusdec929b1 B 
			    LEFT JOIN cusdec929a1 A
			        ON A.SN = B.SNA1
			        AND A.BRANCH = B.BRANCH
			        AND A.CUS_PRG = B.CUS_PRG
			        AND A.RPT_NO = B.RPT_NO
			    LEFT JOIN origin_rates C
			        ON B.HS = C.item_cd
			    WHERE 
			        B.GS_DIVI LIKE 'F%'
		        AND A.Lis_Day BETWEEN #{srch1} AND #{srch2}
		    	<if test="list != null and list.size() > 0">
		            AND A.Nab_SdNo IN 
		            <foreach item="id" collection="list" open="(" separator="," close=")">
		                #{id}
		            </foreach>
		       	</if>
		    GROUP BY B.GS_DIVI
		)
		SELECT * FROM base
		UNION ALL
			SELECT
			    '합계',
			    '',
			    SUM(CNT),
			    SUM(FTA_RATE),
			    SUM(GS),
			    ABS(SUM(DUTY)) AS TOTAL_DUTY
			FROM base
			ORDER BY 
			     CASE WHEN GS_RATE = '합계' THEN 99999 ELSE 1 END,
			    CNT DESC;
	</select>
	
	<select id = "selectFtaSavingAddList" parameterType="searchVO" resultType="egovMap">
		SELECT 
			COUNT(DISTINCT G.RPT_NO) AS CNT 
			,G.GSNAME
			,G.GS_DIVI
			,SUM(G.BEFORE_FTA) AS BEFORE_FTA
			,SUM(G.AFTER_FTA) AS AFTER_FTA
			,SUM(G.SAVING) AS SAVING
			FROM
			(SELECT A.RPT_NO
				,A.Fod_Mark
				,B.Ori_St_Mark1
				,(SELECT Cont FROM CDSTD WHERE Divi = '관세율구분' AND Code = D.Gs_Divi LIMIT 1) AS GSNAME
				,CONCAT(A.Fod_Mark,B.Ori_St_Mark1) AS 적출국원산지
				,B.Gs_Rate AS 기본관세율
				,D.GS_DIVI AS GS_DIVI
				,Gs_Rmv_Mark AS 과세감면부호
				,CONCAT(B.hs, D.GS_DIVI) AS 세번적용가능세종
				,E.TARIFF_RATE AS 적용가능세율
				,ROUND((B.TAX_KRW * B.Gs_Rate) / 100, 0) AS BEFORE_FTA
				,ROUND((B.TAX_KRW * E.TARIFF_RATE)/100,0) AS AFTER_FTA
				,ROUND(((B.Gs_Rate - E.TARIFF_RATE) * B.TAX_KRW)/100,0) AS SAVING
				FROM cusdec929A1 A
				LEFT JOIN cusdec929B1 B
				 ON A.SN = B.SNA1
				 AND A.BRANCH = B.BRANCH
				 AND A.CUS_PRG = B.CUS_PRG
				 AND A.RPT_NO = B.RPT_NO
				 LEFT JOIN origin_tariffrates D
				 ON CONCAT(ifnull(A.Fod_Mark,''),ifnull(B.Ori_St_Mark1,'')) = D.COUNTRY_ORIGIN
				 LEFT JOIN fta_rates E
				 ON CONCAT(B.hs, D.GS_DIVI) = E.ITEMCD_RATE
				 WHERE 1=1
				AND B.Gs_Divi IN ('A','C','F')
				AND A.RPT_SEQ = '00'
				<if test="list != null and list.size() > 0">
		            AND A.Nab_SdNo IN 
		            <foreach item="id" collection="list" open="(" separator="," close=")">
		                #{id}
		            </foreach>
		       	</if>
				AND B.Gs_Rmv_Mark = ''
				AND DATE_ADD(STR_TO_DATE(A.Lis_Day, '%Y%m%d'), INTERVAL 364 DAY) >= CURDATE()
				AND E.TARIFF_RATE  IS NOT NULL
				)G 
			  WHERE 
			  (BEFORE_FTA !=0 || AFTER_FTA != 0 || SAVING != 0) 
			  GROUP BY G.GS_DIVI
			
	</select>
	
