			    SUM(A.Tot_Tax_Sum) AS taxSum,
			    CONCAT(ROUND(SUM(A.Tot_Tax_Sum) / T.total_sum * 100, 2),'%') AS taxRate, -- ✅ 비율(%)
			    1 AS sort_order
			FROM cusdec929a1 A
