	<queryString>
		<![CDATA[SELECT 
		C.CMPNY_NM_EN AS prm1,
		C.TAX_NB AS prm2,
		H.CUSTOM_EXP_NO AS prm3,
		D.BD_RT AS prm4,
		E.PRDCT_VNM AS prm5,
		GET_HSCD(E.HS_CD) AS prm6,
		E.ITEM_CD AS prm7,
		H.SALES_QTY AS prm8,
		H.SALES_PRICE AS prm9,
		D.BD_RT AS prm10,
		DATE_FORMAT(CURDATE(), '%d-%m-%Y') prm11,
		DATE_FORMAT(CURDATE(), '%d') prm12,
		DATE_FORMAT(CURDATE(), '%m') prm13,
		DATE_FORMAT(CURDATE(), '%y') prm14
	FROM 
		jdgmnt_result A 
		LEFT JOIN cmpny_info C
			ON A.CMPNY_CD = C.CMPNY_CD
		LEFT JOIN psr_info D
			ON A.PSR_SEQ = D.PSR_SEQ
		LEFT JOIN item_info E
			ON (A.CMPNY_CD = E.CMPNY_CD AND A.PLNT_CD = E.PLNT_CD AND A.ITEM_CD = E.ITEM_CD)
		LEFT JOIN crft_do_hist F
			ON (A.JDGMNT_SEQ = F.JDGMNT_SEQ AND A.PSR_SEQ = F.PSR_SEQ)
		LEFT JOIN sales_list H
			ON (A.SALES_NO = H.SALES_NO AND A.ITEM_CD = H.ITEM_CD AND A.CMPNY_CD = H.CMPNY_CD) 
	WHERE
	A.JDGMNT_SEQ = $P{i_prm1}
	AND F.ISSU_NO = $P{i_prm4}
	AND A.PLNT_CD = $P{i_prm5}
	AND A.PSR_SEQ = $P{i_prm7}
	
	UNION
	
	SELECT 
	C.CMPNY_NM_EN AS prm1,
	C.TAX_NB AS prm2,
	H.CUSTOM_EXP_NO AS prm3,
	D.BD_RT AS prm4,
	E.PRDCT_VNM AS prm5,
	GET_HSCD(E.HS_CD) AS prm6,
	E.ITEM_CD AS prm7,
	H.SALES_QTY AS prm8,
	H.SALES_PRICE AS prm9,
	D.BD_RT AS prm10,
	DATE_FORMAT(CURDATE(), '%d-%m-%Y') prm11,
	DATE_FORMAT(CURDATE(), '%d') prm12,
	DATE_FORMAT(CURDATE(), '%m') prm13,
	DATE_FORMAT(CURDATE(), '%y') prm14
FROM 
	jdgmnt_result A 
	LEFT JOIN cmpny_info C
	ON A.CMPNY_CD = C.CMPNY_CD
	LEFT JOIN psr_info D
	ON A.PSR_SEQ = D.PSR_SEQ
	LEFT JOIN item_info E
	ON (A.CMPNY_CD = E.CMPNY_CD AND A.PLNT_CD = E.PLNT_CD AND A.ITEM_CD = E.ITEM_CD)
	LEFT JOIN crft_co_hist_dts F
	ON (A.JDGMNT_SEQ = F.JDGMNT_SEQ AND A.PSR_SEQ = F.PSR_SEQ)
	LEFT JOIN sales_list H
	ON (A.SALES_NO = H.SALES_NO AND A.ITEM_CD = H.ITEM_CD AND A.CMPNY_CD = H.CMPNY_CD) 
WHERE
A.JDGMNT_SEQ = $P{i_prm1}
AND F.ISSU_NO = $P{i_prm4}
AND A.PLNT_CD = $P{i_prm5}]]>
	</queryString>
