//검색 버튼 클릭 시 호출되는 함수
function fn_searchCalViewInfo(){
	fn_loading(true);
	var sData = fn_setCalculView();
	
	console.log(sData);
	
	$.ajax({
		type : "POST",
		url : "/rpt/selectCalculInfo2.do",
		data : sData,
		beforeSend : function(xmlHttpRequest){
			xmlHttpRequest.setRequestHeader("AJAX", "true");
		},
		dataType: "json",
        success : function(data) {
        	calculateHot.loadData([]);
        	calculateDetailHot.loadData([]);
        	//calculateDetailHot2.loadData([]);
        	calculateHot.loadData(data.resultList);
        	
        	// 중복된 BL 값 걸러내기
        	const blNoSet = new Set(data.resultList.map(item => item.blNo)); // Set을 사용하여 중복 제거
        	var blNoCount = blNoSet.size; // 고유한 B/L No.의 갯수
        	console.log("blNoCount"+blNoCount);
        	
        	// `supAmt`, `taxAmt`, `totAmt` 합계를 계산
            const sumSupAmt = data.resultList.reduce((total, item) => total + (parseFloat(item.supAmt) || 0), 0);
            const sumTaxAmt = data.resultList.reduce((total, item) => total + (parseFloat(item.taxAmt) || 0), 0);
            const sumTotAmt = data.resultList.reduce((total, item) => total + (parseFloat(item.totAmt) || 0), 0);
        	
        	// 합계 행 설정
        	var sumRow = {
                    blNo: "합계", // Summary label
                    rptNo: blNoCount + "건", // Unique B/L No. count
                    rptDay: "",
                    lisDay: "",
                    partnCmpny: "",
                    description: "",
                    supAmt: sumSupAmt.toLocaleString(),  // 합계 결과를 표시
                    taxAmt: sumTaxAmt.toLocaleString(),  // 합계 결과를 표시
                    totAmt: sumTotAmt.toLocaleString(),  // 합계 결과를 표시
                    remarks: "",
                    paymentYn: ""
                };

        	// 합계 행을 데이터 리스트에 추가
            data.resultList.push(sumRow); // sumRow를 resultList에 추가
            // 새 데이터를 Handsontable에 로드
            calculateHot.loadData(data.resultList); // 새 데이터로 테이블 업데이트
            
            // 합계 행 스타일 설정 (예: 중앙 정렬)
            const lastRowIndex = data.resultList.length - 1; // 마지막 행 인덱스
            calculateHot.updateSettings({
                mergeCells: [
                    { row: lastRowIndex, col: 1, rowspan: 1, colspan: 5 } // rptNo부터 description까지 열을 병합
                ]
            });
            calculateHot.setCellMeta(lastRowIndex, 0, 'className', 'htCenter boldText'); // blNo (0번 열) bold 처리
            calculateHot.setCellMeta(lastRowIndex, 1, 'className', 'htCenter boldText'); // rptNo (1번 열) bold 처리
            calculateHot.setCellMeta(lastRowIndex, 6, 'className', 'boldText htRight');  // supAmt 열
            calculateHot.setCellMeta(lastRowIndex, 7, 'className', 'boldText htRight');  // taxAmt 열
            calculateHot.setCellMeta(lastRowIndex, 8, 'className', 'boldText htRight');  // totAmt 열
            
            console.log("33");
            // 테이블 렌더링
            calculateHot.render();
        	
        	var totCnt = (data.resultList.length > 0) ? data.resultList.length : 0;
        	console.log("data"+data.resultList.length);
        	console.log("totCnt"+totCnt);
        	$("#calculateCnt").text(totCnt-1); //검색결과 총 갯수
        	
        	fn_loading(false);
        },
        error : function(e, textStatus, errorThrown) {
        	if(e.status == 400){
        		alert("Your request is up. Please log back in if you wish continue");
        		location.href = document.referrer;
        	} else {
	        	console.log(errorThrown);
	        	alert(msgSearchError);
        	}
        }
	});
};


