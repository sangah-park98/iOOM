	<queryString>
		<![CDATA[SELECT 
					a.cusNm1 AS prm1,
					DATE_FORMAT(STR_TO_DATE(a.nabDay, '%Y%m%d'), '%Y년 %m월') AS prm2,
					CONCAT_WS('-', 
					        SUBSTRING(a.nabSdNo, 1, 5),
					        SUBSTRING(a.nabSdNo, 6, 2),
					        SUBSTRING(a.nabSdNo, 8)) AS prm3
					
				FROM vatbil5fza1 a
				LEFT JOIN vatbil5fzb1 b
				ON a.BRANCH = b.BRANCH
				AND a.CUS_PRG = b.CUS_PRG
				AND a.docCode = b.docCode
				WHERE a.impCd = REPLACE($P{i_prm1},'-','')
				AND a.NabSdNo = $P{i_prm2}
				LIMIT 1]]>
	</queryString>
	<field name="prm1" class="java.lang.String"/>
	<field name="prm2" class="java.lang.String"/>
	<field name="prm3" class="java.lang.String"/>
