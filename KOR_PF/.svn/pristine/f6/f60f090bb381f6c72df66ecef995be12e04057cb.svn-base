	<parameter name="i_prm1" class="java.lang.String"/>
	<parameter name="i_prm2" class="java.lang.String"/>
	<parameter name="i_prm4" class="java.lang.String"/>
	<parameter name="i_prm5" class="java.lang.String"/>
	<parameter name="i_prm7" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
			GET_DATE(D.PURCHASE_DT,'vt','/') AS d1,
			IFNULL(C.VNDR_NM,'') AS d2,
			IFNULL(C.addr1,'') AS d3,
			IFNULL(C.CORP_NO,'') AS d4,
			IFNULL(E.PRDCT_VNM,'') AS d5,
			IFNULL(CONCAT(GET_HSCD(REPLACE(A.HS_CD,'.',''))),'') AS d6,
			'' AS d7,
			CONCAT(IFNULL(A.INPT_ALL_QY,''), '/', IFNULL(E.BASE_UNT,'')) AS d8,
			IFNULL(FORMAT(ROUND(E.STNDRD_PRICE,2),2),'') AS d9,
			IFNULL(ROUND(D.PURCHASE_PRICE_VN,2),0) AS d10,
			'' AS d11
		FROM 
			jdgmnt_rawmtrl A 
			INNER JOIN crft_do_hist B
				ON (A.JDGMNT_SEQ = B.JDGMNT_SEQ AND A.PSR_SEQ = B.PSR_SEQ)
			LEFT JOIN purchs_list D
				ON (
					A.PURCHASE_NO = D.PURCHASE_NO 
					AND A.PURCHASE_ORDR = D.PURCHASE_ORDR
					AND A.VNDR_CD = D.VNDR_CD
					AND A.PLNT_CD = D.PLNT_CD
					AND A.RAW_ITEM_CD = D.ITEM_CD
				)
			LEFT JOIN partn_vndr_info C
				ON (D.VNDR_CD = C.VNDR_ID)
			LEFT JOIN item_info E
				ON (
					A.CMPNY_CD = E.CMPNY_CD
					AND A.PLNT_CD = E.PLNT_CD
					AND A.RAW_ITEM_CD = E.ITEM_CD
				)
		WHERE 
			A.JDGMNT_SEQ = $P{i_prm1}
			AND B.ISSU_NO = $P{i_prm4}
			AND A.PLNT_CD = $P{i_prm5}
			AND A.PSR_SEQ = $P{i_prm7}
		
		UNION
		
		SELECT 
			GET_DATE(D.PURCHASE_DT,'vt','/') AS d1,
			IFNULL(C.VNDR_NM,'') AS d2,
			IFNULL(C.addr1,'') AS d3,
			IFNULL(C.CORP_NO,'') AS d4,
			IFNULL(E.PRDCT_VNM,'') AS d5,
			IFNULL(CONCAT(GET_HSCD(REPLACE(A.HS_CD,'.',''))),'') AS d6,
			'' AS d7,
			CONCAT(IFNULL(A.INPT_ALL_QY,''), '/', IFNULL(E.BASE_UNT,'')) AS d8,
			IFNULL(FORMAT(E.STNDRD_PRICE,2),'') AS d9,
			IFNULL(FORMAT(D.PURCHASE_PRICE_VN,2),'') AS d10,
			'' AS d11
		FROM 
			jdgmnt_rawmtrl A 
			INNER JOIN crft_co_hist_dts B
			ON (A.JDGMNT_SEQ = B.JDGMNT_SEQ AND A.PSR_SEQ = B.PSR_SEQ)
			LEFT JOIN purchs_list D
			ON (
				A.PURCHASE_NO = D.PURCHASE_NO 
				AND A.PURCHASE_ORDR = D.PURCHASE_ORDR
				AND A.VNDR_CD = D.VNDR_CD
				AND A.PLNT_CD = D.PLNT_CD
				AND A.RAW_ITEM_CD = D.ITEM_CD
				)
			LEFT JOIN partn_vndr_info C
			ON (D.VNDR_CD = C.VNDR_ID)
			LEFT JOIN item_info E
			ON (
				A.CMPNY_CD = E.CMPNY_CD
				AND A.PLNT_CD = E.PLNT_CD
				AND A.RAW_ITEM_CD = E.ITEM_CD
				)
		WHERE  
			A.JDGMNT_SEQ = $P{i_prm1}
			AND A.CMPNY_CD = $P{i_prm2}
			AND B.ISSU_NO = $P{i_prm4}
			AND A.PLNT_CD = $P{i_prm5}
			AND A.PSR_SEQ = $P{i_prm7}]]>
	</queryString>
