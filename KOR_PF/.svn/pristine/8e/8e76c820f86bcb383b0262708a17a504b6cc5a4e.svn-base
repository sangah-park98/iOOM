      $(".kpi-management-view").fadeIn(200);
      kpiSummaryTable.render()
      kpiImportTable.render()
      kpiExportTable.render()
    }
  })
})
kpiSubViewMode.forEach((el) => {
  el.addEventListener('change', () => {
    if (el.value === "kpi-sub-import") {
      $(".kpi-management-import-view").fadeIn(200);
      $(".kpi-management-export-view").fadeOut(0);
      kpiImportTable.render()
    } else if(el.value === "kpi-sub-export") {
