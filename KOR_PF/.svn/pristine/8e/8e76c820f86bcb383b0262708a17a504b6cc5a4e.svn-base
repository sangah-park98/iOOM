const kpiSubViewMode = document.querySelectorAll('input[name="kpi-sub-view"]');
kpiViewMode.forEach((el) => {
  el.addEventListener('change', () => {
    if (el.value === "kpi-leadtime-view") {
      $(".kpi-leadtime-view").fadeIn(200);
      $(".kpi-management-view").fadeOut(0);
      kpiLeadtimeTable.render()
    } else if(el.value === "kpi-management-view") {
