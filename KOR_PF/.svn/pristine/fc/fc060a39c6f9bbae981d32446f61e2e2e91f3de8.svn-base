<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<!doctype html>
<html lang="ko">
<head>
  <meta charset="UTF-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title>수출 Invoice 생성</title>
  <script src="/pf/js/function.js"></script>
  <script src="/pf/js/export/exportMakeIn.js" charset="UTF-8"></script>
 <script type="module" crossorigin src="/pf/assets/dropdown-init.js"></script>
</head>
<body class="flex flex-col max-h-fit">
 <main class="p-2 grow flex flex-col gap-2">
    <h2 class="shrink-0 flex items-center gap-1"><i class="fa-duotone fa-box-open-full text-primary-900"></i>INVOICE 생성</h2>
    <form onkeydown="return preventFormSubmit(event)"
      class="w-full shrink-0 grid grid-rows-4 grid-cols-11 auto-rows-auto
      dark:bg-slate-800 shadow-sm rounded-lg border border-slate-200 dark:border-slate-700
      px-4 py-3 gap-1 z-1 text-base *:text-base items-start">
      <!-- SHIPPER / EXPORTER -->
      <div class="group row-span-2 col-span-7 flex flex-col items-start justify-start gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
        <label for="shipper" class="uppercase text-lg font-semibold text-gray-900">
          * seller / exporter
        </label>
        <input type="text" id="shipper"
          class="w-full h-10 bg-gray-50 border border-gray-300 text-gray-900
          rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
          placeholder="SELLER / EXPORTER" pattern="[a-zA-Z0-9]" oninput="this.value = this.value.replace(/[ㄱ-ㅎㅏ-ㅣ가-힣]/g, '')">
      </div>
      
      <div class="group row-start-3 row-span-2 col-span-7 flex flex-col items-start justify-start gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
		  <label for="address" class="uppercase text-lg font-semibold text-gray-900">
		    * seller / exporter address
		  </label>
		  <textarea type="text" id="address" style="resize: none"
		    class="w-full h-32 bg-gray-50 border border-gray-300 text-gray-900
		    rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
		    placeholder="SELLER / EXPORTER ADDRESS"
		    oninput="this.value = this.value.replace(/[ㄱ-ㅎㅏ-ㅣ가-힣]/g, '')"></textarea>
		</div>
      
      <!-- Consignee -->
      <div class="group row-start-5 row-span-2 col-start-1 col-span-7 flex flex-col items-start justify-start gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
        <label for="consignee" class="uppercase text-lg font-semibold text-gray-900">
          * buyer / importer
        </label>
        <input type="text" id="consignee"
          class="w-full h-10 bg-gray-50 border border-gray-300 text-gray-900
          rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
          placeholder="BUYER / IMPORTER" pattern="[a-zA-Z0-9]" oninput="this.value = this.value.replace(/[ㄱ-ㅎㅏ-ㅣ가-힣]/g, '')">
      </div>
      
      <!-- Invoice Data -->
      <div class="row-span-2 col-start-8 col-span-4 flex flex-col gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
        <label for="invoice-no" class="uppercase text-lg font-semibold text-gray-900">
          * invoice no.
        </label>
        <input type="text" id="invoice-no" oninput="this.value = this.value.replace(/[ㄱ-ㅎㅏ-ㅣ가-힣]/g, '')"
          class="w-full h-10 bg-gray-50 border border-gray-300 text-gray-900
          rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
          placeholder="INVOICE NO."
        >
      </div>
      <!-- Invoice Date -->
	 <div
	      class="row-span-2 col-start-8 col-span-4 flex flex-col gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
	      <div class="w-full flex flex-col gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
       <label for="invoice-date" class="uppercase text-lg font-semibold text-gray-900">
            * invoice Date.
       </label>
      <div class="relative">
        <div class="absolute inset-y-0 start-0 flex items-center ps-3.5 pointer-events-none">
          <svg class="w-4 h-4 text-gray-500 dark:text-gray-400" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 20 20">
            <path d="M20 4a2 2 0 0 0-2-2h-2V1a1 1 0 0 0-2 0v1h-3V1a1 1 0 0 0-2 0v1H6V1a1 1 0 0 0-2 0v1H2a2 2 0 0 0-2 2v2h20V4ZM0 18a2 2 0 0 0 2 2h16a2 2 0 0 0 2-2V8H0v10Zm5-8h10a1 1 0 0 1 0 2H5a1 1 0 0 1 0-2Z"/>
          </svg>
        </div>
         <input type="text" class="form-control band-calendar text-base bg-gray-50 border border-gray-300 text-gray-900 rounded-lg focus:ring-primary-500 focus:border-primary-500 block w-full ps-10 py-1 px-2.5  dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400"
				id="invoiceDate" name="exportInvoice_date"
				onKeypress="javascript:if(event.keyCode==13) {$('.calendar-popup-container').removeClass('calendar-popup-container_active'); $(this).blur()}" onkeyUp="fn_dateInputForm($(this))" 
				placeholder="INVOICE DATE" autocomplete="off">   
        
      </div>
      </div>
    
    </div>
      <div class="row-span-2 col-start-8 col-span-4 flex flex-col gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
        <label for="invoice-to" class="uppercase text-lg font-semibold text-gray-900">
          invoice to.
        </label>
        <input type="text" id="invoiceTo"
          class="w-full h-10 bg-gray-50 border border-gray-300 text-gray-900
          rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
          placeholder="INVOICE TO."
        >
      </div>
       <div class="group row-start-7 row-span-2 col-span-7 flex flex-col items-start justify-start gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
		  <label for="address" class="uppercase text-lg font-semibold text-gray-900">
		    * consignee address 
		  </label>
		  <textarea type="text" id="consigneeAddress" style="resize: none"
		    class="w-full h-32 bg-gray-50 border border-gray-300 text-gray-900
		    rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
		    placeholder="CONSIGNEE ADDRESS"
		    oninput="this.value = this.value.replace(/[ㄱ-ㅎㅏ-ㅣ가-힣]/g, '')"></textarea>
		</div>
      <!-- line -->
      <div class="col-span-11 pt-1 mx-2 mb-1 border-b"></div>
      <!-- Loading Port -->
       <div class="h-full row-span-2 col-start-1 col-span-4 flex items-end">
        <div class="w-full flex flex-col gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
          <label
            for="loading-port"
            class="uppercase text-lg font-semibold text-gray-900"
          >
             * loading port
          </label>
          <div class="relative w-full col-span-2">
            <button
              id="loadingPortButton"
              onclick="fn_loadPort()"
              data-dropdown-toggle="loadingPortSearch"
              data-dropdown-placement="bottom"
              class="h-10 w-full text-primary-900 border border-primary-700 hover:text-white bg-primary-100 hover:bg-primary-500 focus:ring focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center justify-between dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800"
              type="button" 
            >
              Loading Port
              <svg class="w-2.5 h-2.5 ms-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 10 6">
                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 4 4 4-4"/>
              </svg>
            </button>
            <div id="loadingPortSearch" class="z-10 hidden bg-white border rounded-lg shadow-xl w-full dark:bg-gray-700 overflow-auto">
                <div class="p-3">
                  <label for="final-group-search" class="sr-only">Search</label>
                  <div class="relative">
                    <div class="absolute inset-y-0 rtl:inset-r-0 start-0 flex items-center ps-3 pointer-events-none">
                      <svg class="w-4 h-4 text-gray-500 dark:text-gray-400" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20">
                        <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z"/>
                      </svg>
                    </div>
                    <input type="text" id="loadPortSrch1" onkeyUp="enterkeyLoadPort()" class="block w-full p-2 ps-10 text-sm text-gray-900 border border-gray-300 rounded-lg bg-gray-50 focus:ring-primary-500 focus:border-primary-500 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500" placeholder="search">
                  </div>
                </div>
                <ul class="h-48 px-3 pb-3 overflow-y-auto text-sm text-gray-700 dark:text-gray-200 max-h-40 overflow-auto" aria-labelledby="finalSearch"></ul>
            </div>
          </div>
        </div>
      </div>
      
      <!-- Vessel / Flight -->
      <div class="h-full row-span-2 col-start-5 col-span-3 flex items-end">
        <div class="w-full flex flex-col gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
          <label for="vessel" class="uppercase text-lg font-semibold text-gray-900">
            * vessel / flight
          </label>
          <input type="text" id="vessel"
            class="w-full h-10 bg-gray-50 border border-gray-300 text-gray-900 
            rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
            placeholder="Vessel / Flight"
            pattern="[a-zA-Z0-9]"
            oninput="this.value = this.value.replace(/[ㄱ-ㅎㅏ-ㅣ가-힣]/g, '')"
          >
        </div>
      </div>
      <!-- Terms -->
      <div class="row-span-1 col-start-8 col-span-4 grid grid-cols-3 p-1 items-center focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
        <label for="terms-of-payment" class="uppercase text-lg font-semibold text-gray-900">
          * terms of payment
        </label>
        <input type="text" id="termsOfPayment"
          class="col-span-2 w-full h-10 bg-gray-50 border border-gray-300 text-gray-900
          rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
          placeholder="TERMS OF PAYMENT"
        >
      </div>
      
      <div class="row-span-1 col-start-8 col-span-4 grid grid-cols-3 p-1 items-center focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
        <label for="termsOfTrade" class="uppercase text-lg font-semibold text-gray-900">
          * terms of trade
        </label>
        <!-- Dropdown -->
        <div class="relative w-full col-span-2">
          <button
            id="termsOfTradeButton"
            onclick="fn_termsTrade()"
            data-dropdown-toggle="termsOfTradeSearch"
            data-dropdown-placement="bottom"
            class="h-10 w-full text-primary-900 border border-primary-700 hover:text-white bg-primary-100 hover:bg-primary-500 focus:ring focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center justify-between dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800"
            type="button"
          >
            Terms of Trade
            <svg class="w-2.5 h-2.5 ms-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 10 6">
              <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 4 4 4-4"/>
            </svg>
          </button>
          <div id="termsOfTradeSearch" class="z-10 hidden bg-white border rounded-lg shadow-xl w-full dark:bg-gray-700 overflow-auto">
            <div class="p-3">
              <label for="tot-group-search" class="sr-only">Search</label>
              <div class="relative">
                <div class="absolute inset-y-0 rtl:inset-r-0 start-0 flex items-center ps-3 pointer-events-none">
                  <svg class="w-4 h-4 text-gray-500 dark:text-gray-400" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20">
                    <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z"/>
                  </svg>
                </div>
                <input type="text" id="tradeSrch1" onkeyUp="enterkeyTrade()" class="block w-full p-2 ps-10 text-sm text-gray-900 border border-gray-300 rounded-lg bg-gray-50 focus:ring-primary-500 focus:border-primary-500 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500" placeholder="Search">
              </div>
            </div>
            <ul class="h-48 px-3 pb-3 overflow-y-auto text-sm text-gray-700 dark:text-gray-200 max-h-40 overflow-auto" aria-labelledby="termsOfTradeButton">
                <li>
                  <button 
                  	class="block ps-2 hover:bg-gray-100 dark:hover:bg-gray-600 w-full py-2 text-sm font-medium text-gray-900 rounded dark:text-gray-300 text-left">
                  </button>
                </li>
            </ul>
          </div>
        </div>
        <!-- / Dropdown -->
      </div>
      <!-- Final Destination -->
	  <div class="h-full row-span-2 col-start-1 col-span-4 flex items-end">
  	    <div class="w-full flex flex-col gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
	      <label for="final-destination" class="uppercase text-lg font-semibold text-gray-900">
	      * final destination
	      </label>
          <div class="relative w-full col-span-2">
		      <button
		        id="finalButton"
		        onclick="fn_finalPopup()"
		        class="h-10 w-full text-primary-900 border border-primary-700 hover:text-white bg-primary-100 hover:bg-primary-500 focus:ring focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center justify-between dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800"
		        type="button">
		        Final Destination
		        <svg class="w-2.5 h-2.5 ms-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 10 6">
		          <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 4 4 4-4"/>
		        </svg>
		      </button>
	      <div id="popup" class="hidden absolute top-0 left-0 right-0 bottom-0 bg-white border rounded-lg shadow-xl dark:bg-gray-700 overflow-auto">
	        <!-- 팝업 내용 -->
	        <div class="p-3">
	          <label for="final-group-search" class="sr-only">Search</label>
	          <div class="relative">
	            <div class="absolute inset-y-0 rtl:inset-r-0 start-0 flex items-center ps-3 pointer-events-none">
	              <svg class="w-4 h-4 text-gray-500 dark:text-gray-400" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20">
	                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z"/>
	              </svg>
	            </div>
	          </div>
	        </div>
		        <ul 
		        	class="h-48 px-3 pb-3 overflow-y-auto text-sm text-gray-700 dark:text-gray-200 max-h-40 overflow-auto" aria-labelledby="termsOfTradeButton">
		        </ul>
		      </div>
		    </div>
		  </div>
		</div>
      
      <!-- Departure Date -->
	  <div
	      class="h-full row-span-2 col-start-5 col-span-3 flex items-end">
	      <div class="w-full flex flex-col gap-1 p-1 focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
       <label for="departureDate" class="uppercase text-lg font-semibold text-gray-900">
           * departure date
       </label>
      <div class="relative">
        <div class="absolute inset-y-0 start-0 flex items-center ps-3.5 pointer-events-none">
          <svg class="w-4 h-4 text-gray-500 dark:text-gray-400" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 20 20">
            <path d="M20 4a2 2 0 0 0-2-2h-2V1a1 1 0 0 0-2 0v1h-3V1a1 1 0 0 0-2 0v1H6V1a1 1 0 0 0-2 0v1H2a2 2 0 0 0-2 2v2h20V4ZM0 18a2 2 0 0 0 2 2h16a2 2 0 0 0 2-2V8H0v10Zm5-8h10a1 1 0 0 1 0 2H5a1 1 0 0 1 0-2Z"/>
          </svg>
        </div>
        <input type="text" class="form-control band-calendar text-base bg-gray-50 border border-gray-300 text-gray-900 rounded-lg focus:ring-primary-500 focus:border-primary-500 block w-full ps-10 py-1 px-2.5  dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400"
				id="inDepDate" name="exportDeparture_date"
				onKeypress="javascript:if(event.keyCode==13) {$('.calendar-popup-container').removeClass('calendar-popup-container_active'); $(this).blur()}" onkeyUp="fn_dateInputForm($(this))" 
				placeholder="DEPARTURE DATE" autocomplete="off">   
      </div>
      </div>
    </div>
      <!-- Freight -->
      <div class="row-span-1 col-start-8 col-span-4 grid grid-cols-333 p-1 items-center focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
        <label for="freight" class="uppercase text-lg font-semibold text-gray-900">
          freight
        </label>
        <input type="text" id="freight" oninput="this.value = this.value.replace(/[^\d]/g, '').replace(/\B(?=(\d{3})+(?!\d))/g, ',')"
          class="h-10 bg-gray-50 border border-gray-300 text-gray-900
          rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
          placeholder="FREIGHT"
        >
        <button
            id="freightCurrencyButton"
            onclick="fn_freightCurrency()"
            data-dropdown-toggle="freightCurrencySearch"
            data-dropdown-placement="bottom"
            class="h-10 text-primary-900 border border-primary-700 hover:text-white bg-primary-100 hover:bg-primary-500 focus:ring focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center justify-between dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800"
            type="button">
           	 Currency
            <svg class="w-2.5 h-2.5 ms-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 10 6">
              <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 4 4 4-4"/>
            </svg>
          </button>
          <div id="freightCurrencySearch" class="z-10 hidden bg-white border rounded-lg shadow-xl dark:bg-gray-700 overflow-auto">
              <div class="p-3">
                <label for="currency-group-search" class="sr-only">Search</label>
                <div class="relative">
                  <div class="absolute inset-y-0 rtl:inset-r-0 start-0 flex items-center ps-3 pointer-events-none">
                    <svg class="w-4 h-4 text-gray-500 dark:text-gray-400" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20">
                      <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z"/>
                    </svg>
                  </div>
                  <input type="text" id="freightCurrencySrch1" onkeyup="enterkeyFreightCurrency()" class="block w-full p-2 ps-10 text-sm text-gray-900 border border-gray-300 rounded-lg bg-gray-50 focus:ring-primary-500 focus:border-primary-500 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500" placeholder="search">
                </div>
              </div>
              <ul class="h-48 px-3 pb-3 overflow-y-auto text-sm text-gray-700 dark:text-gray-200 max-h-40 overflow-auto" aria-labelledby="freightCurrencySearch">
          		<li>
            	  <button 
              		class="block ps-2 hover:bg-gray-100 dark:hover:bg-gray-600 w-full py-2 text-sm font-medium text-gray-900 rounded dark:text-gray-300 text-left">
                  </button>
                </li>
      		</ul>
          </div>
      </div>
      <!-- Insuarance -->
      <div class="row-span-1 col-start-8 col-span-4 grid grid-cols-333 p-1 items-center focus-within:ring-2 focus-within:ring-primary-500 focus-within:bg-primary-50 rounded">
        <label for="insurance" class="uppercase text-lg font-semibold text-gray-900">
          insurance
        </label>
        <input type="text" id="insurance" oninput="this.value = this.value.replace(/[^\d]/g, '').replace(/\B(?=(\d{3})+(?!\d))/g, ',')"
          class="col-span-1 h-10 bg-gray-50 border border-gray-300 text-gray-900 rounded-lg focus:ring-primary-500 focus:border-primary-500 block px-2.5 py-1"
          placeholder="INSURANCE"
        >
         <div class="relative col-span-1">
          <button
            id="insuranceCurrencyButton"
            onclick="fn_insuranceCurrency()"
            data-dropdown-toggle="insuranceCurrencySearch"
            data-dropdown-placement="bottom"
            class="h-10 w-full text-primary-900 border border-primary-700 hover:text-white bg-primary-100 hover:bg-primary-500 focus:ring focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center justify-between dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800"
            type="button">
            Currency
            <svg class="w-2.5 h-2.5 ms-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 10 6">
              <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 4 4 4-4"/>
            </svg>
          </button>
          <div id="insuranceCurrencySearch" class="z-10 hidden bg-white border rounded-lg shadow-xl dark:bg-gray-700 overflow-auto">
              <div class="p-3">
                <label for="currency-group-search" class="sr-only">Search</label>
                <div class="relative">
                  <div class="absolute inset-y-0 rtl:inset-r-0 start-0 flex items-center ps-3 pointer-events-none">
                    <svg class="w-4 h-4 text-gray-500 dark:text-gray-400" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20">
                      <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z"/>
